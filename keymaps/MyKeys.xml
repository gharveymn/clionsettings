<keymap version="1" name="MyKeys" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="AddToFavoritesPopup" />
  <action id="Editor Redo">
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="EditorCloneCaretAbove">
    <keyboard-shortcut first-keystroke="ctrl up" />
  </action>
  <action id="EditorCloneCaretBelow">
    <keyboard-shortcut first-keystroke="ctrl down" />
  </action>
  <action id="EditorDeleteLine" />
  <action id="EditorLookupDown" />
  <action id="EditorLookupUp" />
  <action id="EditorNextWord" />
  <action id="EditorPreviousWord" />
  <action id="EditorScrollDown" />
  <action id="EditorScrollUp" />
  <action id="MethodOverloadSwitchDown" />
  <action id="MethodOverloadSwitchUp" />
  <action id="NextParameter">
    <keyboard-shortcut first-keystroke="tab" />
    <keyboard-shortcut first-keystroke="ctrl right" />
  </action>
  <action id="NextTemplateVariable">
    <keyboard-shortcut first-keystroke="tab" />
    <keyboard-shortcut first-keystroke="enter" />
    <keyboard-shortcut first-keystroke="ctrl right" />
  </action>
  <action id="PrevParameter">
    <keyboard-shortcut first-keystroke="shift tab" />
    <keyboard-shortcut first-keystroke="ctrl left" />
  </action>
  <action id="PreviousTemplateVariable">
    <keyboard-shortcut first-keystroke="shift tab" />
    <keyboard-shortcut first-keystroke="ctrl left" />
  </action>
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="ctrl alt l" />
    <keyboard-shortcut first-keystroke="shift alt f" />
  </action>
</keymap>